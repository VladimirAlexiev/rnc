namespace a = "http://relaxng.org/ns/compatibility/annotations/1.0"
namespace doc =
  "http://collectionstrust.org.uk/spectrumXML/Documentation"
namespace rng = "http://relaxng.org/ns/structure/1.0"
default namespace spectrum =
  "http://collectionstrust.org.uk/spectrumXML/Schema-v4.0"
namespace xsd = "http://www.w3.org/2001/XMLSchema"

##    Collections Trust XML Schema for SPECTRUM version 4.0.4 (26 June 2012)

start |= starting_Interchange
starting_Interchange = element Interchange {Interchange}

Address =
  element EMail {xsd:string}?,
  element Fax {xsd:string}?,
  element Place {Place}*,
  element Postcode {xsd:string}?,
  element Telephone {xsd:string}?,
  element Text {xsd:string}?,
  element Type {xsd:string}?

Date =
  element Association {xsd:string}?,
  element Earliest {
    element Certainty {xsd:string}?,
    element Date {xsd:string},
    element Qualifier {xsd:string}?}?,
  element Latest {
    element Certainty {xsd:string}?,
    element Date {xsd:string},
    element Qualifier {xsd:string}?}?,
  element Period {xsd:string}?,
  element Text {xsd:string}?

ExactDate =
  element Day {xsd:integer}?,
  element Month {xsd:integer}?,
  element Year {xsd:integer}?

Interchange =
  element Record {
    element Data {
      element Address {Address}
    | element Object {Object}
    | element Organisation {Organisation}
    | element People {People}
    | element Person {Person}
    | element Place {Place}}?}*

Metadata =
  element AmendmentHistory {
    element Change {
      element Authoriser {Person}?,
      element SchemeUsed {xsd:string}?,
      element Source {empty}*,
      element SourceDate {Date}?,
      element UnitAdded {xsd:string}?}+}?,
  element Date {ExactDate}?,
  element Progress {xsd:string}?,
  element Recorder {Person}?,
  element Type {xsd:string}?,
  element Usage {xsd:string}*

Object =
  element AccessCategory {
    element Date {ExactDate}?,
    element Name {xsd:string},
    element Note {xsd:string}?}?,
  element Acquisition {
