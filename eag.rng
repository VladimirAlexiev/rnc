    <rng:define name="relations"><rng:element name="relations">
                <rng:zeroOrMore><rng:ref name="resourceRelation"/></rng:zeroOrMore>
                <rng:zeroOrMore><rng:ref name="eagRelation"/></rng:zeroOrMore>
    </rng:element></rng:define>
    <rng:define name="resourceRelation"><rng:element name="resourceRelation">
                <rng:ref name="relationEntry"/>
                <rng:optional><rng:ref name="descriptiveNote"/></rng:optional>
            <rng:attribute name="resourceRelationType">
                    <rng:choice>
                        <rng:value>creatorOf</rng:value>
                        <rng:value>subjectOf</rng:value>
                        <rng:value>other</rng:value>
                    </rng:choice>
            </rng:attribute>
            <rng:ref name="href"/>
    </rng:element></rng:define>
    <rng:define name="relationEntry"><rng:element name="relationEntry">
    <rng:text/></rng:element></rng:define>
    <rng:define name="eagRelation"><rng:element name="eagRelation">
                <rng:zeroOrMore><rng:ref name="relationEntry"/></rng:zeroOrMore>
                <rng:optional><rng:ref name="descriptiveNote"/></rng:optional>
            <rng:attribute name="eagRelationType">
                    <rng:choice>
                        <rng:value>hierarchical-child</rng:value>
                        <rng:value>hierarchical-parent</rng:value>
                        <rng:value>temporal-earlier</rng:value>
                        <rng:value>temporal-later</rng:value>
                        <rng:value>associative</rng:value>
                    </rng:choice>
            </rng:attribute>
            <rng:ref name="href"/>
    </rng:element></rng:define>
