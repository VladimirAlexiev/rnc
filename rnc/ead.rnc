        element descrules {
          a.common,
          attribute encodinganalog {text}?,
          (text | m.phrase.basic)*}?}?,
      element revisiondesc {
        a.common,
        attribute encodinganalog {text}?,
        (\list
         | element change {
             a.common,
             attribute encodinganalog {text}?,
             date,
             item+}+)}?},
    element frontmatter {
      a.common,
      element titlepage {
        a.common,
        (m.blocks
         | author
         | date
         | edition
         | num
         | publisher
         | bibseries
         | sponsor
         | titleproper
         | subtitle)+}?,
      \div*}?,
    element archdesc {
      a.common,
      attribute level {av.level},
      a.desc.base,
      attribute type {xsd:NMTOKEN}?,
      attribute relatedencoding {text}?,
      element runner {
        a.common,
        attribute placement {"header" | "footer" | "watermark"}?,
        attribute role {text}?,
        (text | m.phrase.bare)*}*,
      did,
      m.desc.full*}}
