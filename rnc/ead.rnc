m.render =
  element emph {
    attribute render {av.render}?,
    attribute id {xsd:ID}?,
    attribute altrender {text}?,
    (text | m.phrase.basic)*}
  | element lb {empty, empty}
m.refs =
  ref
  | extref
  | element linkgrp {a.common, extendedLink, (extended.els)+}
  | bibref
  | title
  | archref
m.access =
  corpname
  | famname
  | geogname
  | name
  | element occupation {
      a.common,
      a.access,
      attribute encodinganalog {text}?,
      (text | m.phrase.bare)*}
  | persname
  | element subject {
      a.common,
      a.access,
      attribute encodinganalog {text}?,
      (text | m.phrase.bare)*}
  | element genreform {
      a.common,
      attribute type {text}?,
      a.access,
      attribute encodinganalog {text}?,
      (text | m.phrase.bare)*}
  | element function {
      a.common,
      a.access,
      attribute encodinganalog {text}?,
      (text | m.phrase.bare)*}
m.access.title = m.access | title
m.data =
  m.access
  | date
  | num
  | origination
  | repository
  | unitdate
  | unittitle
m.phrase.bare =
  (ptr
   | element extptr {a.common, a.external.ptr, empty})
  | m.render
m.phrase.basic.norefs = m.phrase.bare | abbr | expan
m.phrase.basic = m.phrase.basic.norefs | m.refs
m.phrase.plus = m.phrase.basic.norefs | m.data | m.refs
m.inter.noquote =
  address
  | element chronlist {
      a.common,
      attribute encodinganalog {text}?,
      head?,
      listhead?,
      element chronitem {
        a.common,
        date,
        (event
         | element eventgrp {a.common, event+})}+}
  | \list
  | note
  | element table {
      a.common,
      attribute frame {
        "top" | "bottom" | "topbot" | "all" | "sides" | "none"}?,
      attribute colsep {yesorno}?,
      attribute rowsep {yesorno}?,
      attribute pgwide {yesorno}?,
      head?,
      element tgroup {
        a.common,
        attribute cols {xsd:NMTOKEN},
        attribute colsep {yesorno}?,
        attribute rowsep {yesorno}?,
        attribute align {
          "left" | "right" | "center" | "justify" | "char"}?,
        element colspec {
          attribute colnum {xsd:NMTOKEN}?,
          attribute colname {xsd:NMTOKEN}?,
          attribute colwidth {text}?,
          attribute colsep {yesorno}?,
          attribute rowsep {yesorno}?,
          attribute align {
            "left" | "right" | "center" | "justify" | "char"}?,
          attribute char {text}?,
          attribute charoff {xsd:NMTOKEN}?,
          empty}*,
        thead?,
        element tbody {
          a.common,
          attribute valign {"top" | "middle" | "bottom"}?,
          row+}}+}
m.inter =
  m.inter.noquote
  | element blockquote {a.common, (m.inter.noquote | p)+}
m.blocks = m.inter | p
m.did =
  element abstract {
    a.common,
    attribute label {text}?,
    attribute encodinganalog {text}?,
    attribute type {text}?,
    am.langcode,
    (text | m.phrase.basic)*}
  | element container {
      a.common,
      attribute label {text}?,
      attribute type {xsd:NMTOKEN}?,
      attribute encodinganalog {text}?,
      attribute parent {xsd:IDREFS}?,
      (text | m.phrase.basic)*}
