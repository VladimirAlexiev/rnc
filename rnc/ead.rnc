  | daogrp
  | element langmaterial {
      a.common,
      attribute label {text}?,
      attribute encodinganalog {text}?,
      (text | m.phrase.basic | language)*}
  | materialspec
  | note
  | origination
  | element physdesc {
      a.common,
      attribute label {text}?,
      attribute encodinganalog {text}?,
      attribute source {xsd:NMTOKEN}?,
      attribute rules {xsd:NMTOKEN}?,
      (text
       | m.phrase.basic
       | dimensions
       | element physfacet {
           a.common,
           attribute label {text}?,
           attribute type {text}?,
           attribute unit {text}?,
           attribute source {xsd:NMTOKEN}?,
           attribute rules {xsd:NMTOKEN}?,
           attribute encodinganalog {text}?,
           (text | m.phrase.basic | m.access | date)*}
       | element extent {
           a.common,
           attribute label {text}?,
           attribute type {text}?,
           attribute unit {text}?,
           attribute encodinganalog {text}?,
           (text | m.phrase.basic)*}
       | date
       | m.access)*}
  | element physloc {
      a.common,
      attribute label {text}?,
      attribute type {text}?,
      attribute encodinganalog {text}?,
      attribute parent {xsd:IDREFS}?,
      (text | m.phrase.basic)*}
  | repository
  | unitdate
  | element unitid {
      a.common,
      attribute label {text}?,
      attribute type {text}?,
      am.countrycode,
      attribute repositorycode {data.repositorycode}?,
      attribute identifier {text}?,
      attribute encodinganalog {text}?,
      (text | m.phrase.basic)*}
  | unittitle
